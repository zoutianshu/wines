<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.alex.dao.feedback.feedbacks">
    <insert id="insertFeedback" parameterType="feedbacks">
        insert into feedback_table values (#{user_account},#{user_id},#{user_nickname},#{user_problem},#{user_name},#{user_number},#{problem_method})
    </insert>
</mapper>